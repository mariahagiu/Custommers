<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
    </configSections>
    <connectionStrings>
        <add name="Resource.Properties.Settings.CustommerConnectionString"
            connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\Custommer.mdf;Integrated Security=True;Connect Timeout=30"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="ResourceServiceBehavior" name="Resource.Services.CustommerResource">
        <endpoint address="DataServices" binding="netTcpBinding" bindingConfiguration="" contract="Resource.ServiceContracts.ICustommerResource">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="DataServices/mex" binding="mexTcpBinding" bindingConfiguration="" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:8523" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="ResourceServiceBehavior">
          <serviceMetadata httpGetEnabled="false" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>